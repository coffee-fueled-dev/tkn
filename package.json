{
  "name": "@tkn/monorepo",
  "version": "0.1.0-beta.0",
  "private": true,
  "type": "module",
  "workspaces": [
    "packages/*",
    "tools/*",
    "evals"
  ],
  "scripts": {
    "context": "bun run --filter @tkn/scripts context",
    "corpus:tinystories": "bun run --filter @tkn/corpora corpus:tinystories",
    "corpus:brown": "bun run --filter @tkn/corpora corpus:brown",
    "corpus:multi": "bun run --filter @tkn/corpora corpus:multilingual",
    "eval:engsm": "bun run --elide-lines 0 --filter @tkn/evals eval:english-small",
    "eval:engmd": "bun run --elide-lines 0 --filter @tkn/evals eval:english-medium",
    "eval:cross": "bun run --elide-lines 0 --filter @tkn/evals eval:cross-lingual",
    "eval:perf": "bun run --elide-lines 0 --filter @tkn/evals eval:performance"
  },
  "devDependencies": {
    "@types/bun": "latest"
  },
  "peerDependencies": {},
  "trustedDependencies": [
    "kuzu"
  ]
}
