{
  "name": "@tkn/monorepo",
  "version": "0.1.0-beta.0",
  "private": true,
  "type": "module",
  "workspaces": [
    "packages/*",
    "tools/*",
    "studies/*"
  ],
  "scripts": {
    "context": "bun run --filter @tkn/scripts context",
    "corpus:tinystories": "bun run --filter @tkn/corpora corpus:tinystories",
    "corpus:brown": "bun run --filter @tkn/corpora corpus:brown",
    "corpus:multilingual": "bun run --filter @tkn/corpora corpus:multilingual",
    "eval:english-small": "bun run --elide-lines 0 --filter @tkn/studies-tokenizer-training eval:english-small",
    "eval:english-medium": "bun run --elide-lines 0 --filter @tkn/studies-tokenizer-training eval:english-medium",
    "eval:cross-lingual": "bun run --elide-lines 0 --filter @tkn/studies-tokenizer-training eval:cross-lingual"
  },
  "devDependencies": {
    "@types/bun": "latest"
  },
  "peerDependencies": {},
  "trustedDependencies": [
    "kuzu"
  ]
}
